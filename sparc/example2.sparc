let
  type point = PointTwo of Z * Z
             | PointThree of Z * Z * Z
  injectThree (PointTwo (x,y)) = PointThree (x,y,0)
  projectTwo (PointThree (x,y,z)) = PointTwo (x,y)
  compose f g = f g
  p0 = PointTwo (0,0)
  q0 = injectThree p0
  p1 = (compose projectTwo injectThree) p0
in
  (p0,q0)
end